<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
            <record id="res_config_settings_view_form"  model="ir.ui.view">
                <field name="name">res.config.settings.view.form.inherit.stock_module</field>
                <field name="model">res.config.settings</field>
                <field name="inherit_id" ref="stock.res_config_settings_view_form"/> 
                <field name="arch" type="xml">
                    <xpath expr="//app[@name='stock']" position="inside">
                        <block title="Stock Module" name="stock_module_setting_container">
                            <setting id="stock_transport" string="Stock Module" help="Help to install stock module">
                                <field name="module_stock_transport"/>
                            </setting>
                        </block>
                    </xpath>

                </field>
            </record>
    </data>
</odoo>


