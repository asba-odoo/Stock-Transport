<?xml version="1.0" encoding="UTF-8"?>
<odoo>


    <record id="dock_stock_view_form" model="ir.ui.view">
        <field name="name">dock.model.view.form</field>
        <field name="model">dock.model</field>
        <field name="arch" type="xml">
            <form string="test">
                <sheet>
                    <group>
                        <group>
                            <field name="name" />
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

  
    <record id="action_add_dock" model="ir.actions.act_window">
        <field name="name">Dock</field>
        <field name="res_model">dock.model</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create Dock Details
            </p> 
        </field>
    </record>

   
    <menuitem id="dock_menu" name="Stock Transport" />
    <menuitem id="add_dock_menu" name="Dock" action="action_add_dock" parent="dock_menu"/>
  
</odoo>